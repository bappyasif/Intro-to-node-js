<!DOCTYPE html>
<html lang="en">
    <%- include("./partials/head.ejs") %>
<body>
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-2">
                <%- include("./partials/nav.ejs") %>
            </div>
            <div class="col">
                <h1><%= title %> </h1>
                <ul>
                    <% author_list.forEach(author => { %>
                        <!-- <li><a href=<\%= author.url %> ><\%= author.name %> </a> (<\%= author.date_of_birth %> -- <\%= author.date_of_death %> ) </li> -->
                        <!-- <li><a href=<\%= author.url %> ><\%= author.name %> </a> (<\%= author.lifespan %> ) </li> -->
                        <!-- <li><a href=<\%= author.url %> ><\%= author.name %> </a> (<\%= author.formatted_dd %> ) </li> -->
                        <li><a href=<%= author.url %> ><%= author.name %> </a> (<%= author.lifespan %> ) </li>
                    <% }) %>
